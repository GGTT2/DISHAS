	{% endblock %}
</head>

<body>

	{% include 'TAMASALFABundle:Default:navbar.html.twig' %}

	<div class="row alfa-content">
		<div class="col-md-12">
			{% block main %}
			{% endblock %}
		</div>
	</div>
</body>

<footer class="container-fluid text-center"></footer>
{% block javascripts %}
	{% include '/includes/javascripts.html.twig' %}
{% endblock %}