	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<link rel="icon" type="image/x-icon" href="{{ asset('logo/favicon-ALFA.jpg') }}" />
	<link rel="stylesheet" href="{{ asset('css/ALFA.css')}}">

	<title>
		{% block title %}
			ALFA - Manuscript survey
		{% endblock %}
