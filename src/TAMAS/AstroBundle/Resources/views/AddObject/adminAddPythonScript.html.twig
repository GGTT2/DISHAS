{% block tamasastro_body %}
    {% form_theme form 'bootstrap_3_TAMAS.html.twig'
'jquery.collection.html.twig' %}
<h3>Add python script <small>{{pythonScript.scriptName}}</small></h3>
    <div class="row">
        <div class="col-md-10">
            <div class="well">
                {{ form_start(form) }}
                {{form_row(form.scriptFile)}}
                {{form_row(form.scriptUserName)}}
                {{form_row(form.comment)}}
            </div>
        </div>
        <div class="col-md-2">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <h4>Admin board</h4>
                </div>
                <div class="panel-body">
                    {{ form_rest(form) }}
                    {{ form_end(form)}}
                </div>
            </div>
        </div>
    </div>
{% endblock%}

{% block javascripts %}
